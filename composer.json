{
    "name": "symplify/phpstan-extensions",
    "type": "phpstan-extension",
    "description": "Pre-escaped error messages in 'symplify' error format, container aware test case and other useful extensions for PHPStan",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "phpstan/phpstan": "^1.3",
        "symplify/package-builder": "^10.1",
        "symplify/smart-file-system": "^10.1",
        "symplify/astral": "^10.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PHPStanExtensions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\PHPStanExtensions\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "10.1-dev"
        },
        "phpstan": {
            "includes": [
                "config/config.neon"
            ]
        }
    },
    "conflict": {
        "symplify/composer-json-manipulator": "<10.0.8",
        "symplify/easy-coding-standard": "<10.0.8",
        "symplify/phpstan-rules": "<10.0.8",
        "symplify/easy-testing": "<10.0.8",
        "symplify/rule-doc-generator-contracts": "<10.0.8",
        "symplify/symfony-php-config": "<10.0.8",
        "symplify/simple-php-doc-parser": "<10.0.8",
        "symplify/php-config-printer": "<10.0.8",
        "symplify/console-color-diff": "<10.0.8",
        "symplify/autowire-array-parameter": "<10.0.8",
        "symplify/markdown-diff": "<10.0.8",
        "symplify/amnesia": "<10.0.8",
        "symplify/rule-doc-generator": "<10.0.8",
        "symplify/vendor-patches": "<10.0.8",
        "symplify/skipper": "<10.0.8",
        "symplify/symfony-static-dumper": "<10.0.8",
        "symplify/git-wrapper": "<10.0.8",
        "symplify/symplify-kernel": "<10.0.8",
        "symplify/monorepo-builder": "<10.0.8",
        "symplify/config-transformer": "<10.0.8",
        "symplify/easy-ci": "<10.0.8",
        "symplify/coding-standard": "<10.0.8",
        "symplify/latte-phpstan-compiler": "<10.0.8",
        "symplify/template-phpstan-compiler": "<10.0.8",
        "symplify/phpstan-latte-rules": "<10.0.8",
        "symplify/easy-parallel": "<10.0.8",
        "symplify/neon-config-dumper": "<10.0.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
