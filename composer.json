{
    "name": "symplify/phpstan-extensions",
    "type": "phpstan-extension",
    "description": "Pre-escaped error messages in 'symplify' error format, container aware test case and other useful extensions for PHPStan",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "phpstan/phpstan": "^0.12.99",
        "symplify/package-builder": "^9.4.69",
        "symplify/smart-file-system": "^9.4.69",
        "symplify/astral": "^9.4.69"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PHPStanExtensions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\PHPStanExtensions\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.5-dev"
        },
        "phpstan": {
            "includes": [
                "config/config.neon"
            ]
        }
    },
    "conflict": {
        "symplify/composer-json-manipulator": "<9.4.69",
        "symplify/easy-coding-standard": "<9.4.69",
        "symplify/phpstan-rules": "<9.4.69",
        "symplify/easy-testing": "<9.4.69",
        "symplify/psr4-switcher": "<9.4.69",
        "symplify/rule-doc-generator-contracts": "<9.4.69",
        "symplify/symfony-php-config": "<9.4.69",
        "symplify/simple-php-doc-parser": "<9.4.69",
        "symplify/php-config-printer": "<9.4.69",
        "symplify/console-color-diff": "<9.4.69",
        "symplify/autowire-array-parameter": "<9.4.69",
        "symplify/markdown-diff": "<9.4.69",
        "symplify/amnesia": "<9.4.69",
        "symplify/rule-doc-generator": "<9.4.69",
        "symplify/console-package-builder": "<9.4.69",
        "symplify/vendor-patches": "<9.4.69",
        "symplify/skipper": "<9.4.69",
        "symplify/easy-hydrator": "<9.4.69",
        "symplify/symfony-static-dumper": "<9.4.69",
        "symplify/git-wrapper": "<9.4.69",
        "symplify/symplify-kernel": "<9.4.69",
        "symplify/monorepo-builder": "<9.4.69",
        "symplify/config-transformer": "<9.4.69",
        "symplify/easy-ci": "<9.4.69",
        "symplify/coding-standard": "<9.4.69",
        "symplify/twig-phpstan-compiler": "<9.4.69",
        "symplify/latte-phpstan-compiler": "<9.4.69"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
